
<div class="upload-container">
    <form action="/upload/upload_file" method="post" class="file-upload" enctype="multipart/form-data">
        <h1>Upload a File (1 file at a time) <p class="exit-file-popout" >x</p></h1>
        <input type="file" name="file" id="file" required>
        <button type="submit" class="submit-file">Upload</button>
    </form>  
</div>

<div class="new-folder-container">
    <form action="/upload/upload_folder" method="post" class="file-upload">
        <h1>Create new folder <p class="exit-folder-popout">x</p></h1>
        <input type="text" name="newFolderName" required>
        <button type="submit" class="submit-file">Create</button>
    </form>
</div> 

